# Navigation
alias ..='cd ..'
alias .2='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'

# Dotfiles
alias balias='vim ~/.bash_aliases'
alias bashrc='vim ~/.bashrc'
alias sobash='source ~/.bashrc && echo "wow"'  # very entendre
alias gitconf='vim ~/.gitconfig'

# Command aliases
alias c='cd'
alias f='fg'
alias g='git'
alias v='vim'
